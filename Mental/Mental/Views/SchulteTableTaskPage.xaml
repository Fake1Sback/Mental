<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Mental.Views.SchulteTableTaskPage">
    <ContentPage.Content>
        <StackLayout>
            <Label x:Name="Timer_Label" FontSize="Large" Text="{Binding TimerValue}" HorizontalTextAlignment="Center"></Label>
            <Label x:Name="CurrentNumberLabel" Text="{Binding CurrentNumberString}" HorizontalOptions="Center" FontSize="Large"></Label>
            <Grid x:Name="SchulteTableGrid" Padding="10,10,10,10" HorizontalOptions="Center" VerticalOptions="Center">
                
            </Grid>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>